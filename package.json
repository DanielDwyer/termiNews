{
  "name": "terminews",
  "version": "2.9.61",
  "description": "An updated version of terminews - read today's headlines from your terminal.",
  "main": "index.js",
  "scripts": {
    "news": "node ./dist/index.js",
    "test": "./node_modules/.bin/mocha",
    "build:commonjs": "babel index.js --out-dir dist/index.js",
    "build:umd": "cross-env FILE_NAME=index webpack",
    "build:umd:min": "cross-env NODE_ENV=production npm run build:umd"
  },
  "bin": {
    "news": "./bin/index.js"
  },
  "preferGlobal": "true",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielDwyer/terminews.git"
  },
  "keywords": [
    "news",
    "newspaper",
    "headlines",
    "current",
    "today",
    "events",
    "terminews"
  ],
  "author": "danielpatrickdwyer@gmail.com",
  "license": "MIT",
  "contributors": [
    {
      "name": "Jennifer Winer",
      "url": "https://jenniferwiner.com",
      "contributions": 1
    }
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "chai": "^4.1.2",
    "cross-env": "^7.0.2",
    "mocha": "^5.2.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
